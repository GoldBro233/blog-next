{
  "name": "Webdev Container",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "runArgs": [
    "--userns=keep-id"
  ],
  "workspaceMount": "source=${localWorkspaceFolder},target=/workspaces/${localWorkspaceFolderBasename},type=bind,Z",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "remoteUser": "vscode"
}